        <shape>
            <corners android:radius="@dimen/button_corner" />
            <solid android:color="@color/bg_grey_press"></solid>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/button_corner" />
            <solid android:color="@color/bg_grey"></solid>
        </shape>
    </item>
</selector>